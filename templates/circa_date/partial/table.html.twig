  <table class="table table-bordered table-condensed table-hover table-striped">
      <thead>
          <tr>
              <th>ID</th>                                       <th>Value</th>
                                                         <th>Start</th>
                                                         <th>Start Circa</th>
                                                         <th>End</th>
                                                         <th>End Circa</th>
                  
          </tr>
      </thead>
      <tbody>
      {% for circa_date in circa_dates %}
          <tr>
              <td>
                  <a href="{{ path('circa_date_show', { 'id': circa_date.id }) }}">
                      {{ circa_date.id }}
                  </a>
              </td>
              <td>
                  {{ circa_date.value }}
              </td>
          
              <td>
                  {{ circa_date.start }}
              </td>
          
              <td>{% if circa_date.startCirca %}Yes{% else %}No{% endif %}</td>

          
          
              <td>
                  {{ circa_date.end }}
              </td>
          
              <td>{% if circa_date.endCirca %}Yes{% else %}No{% endif %}</td>

          
          
          </tr>
      {% endfor %}
      </tbody>
  </table>
  